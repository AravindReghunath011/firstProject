  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.1.5/dist/sweetalert2.min.js"></script>

<script>
    const startingDateInput = document.getElementById("startingDate");
    const todayDate = new Date().toISOString().slice(0, 10);
    document.getElementById('startingDate').value = todayDate
    const todayDateObj = new Date(todayDate)
    function validate(){
        
        const sDate = document.getElementById('startingDate').value
        const eDate = document.getElementById('expiringDate').value
        const sDateObj = new Date(sDate);
        const eDateObj = new Date(eDate)
        if (sDateObj > eDateObj){
            Swal.fire({
                    title: 'Error!',
                    text: 'expiry date should be after the starting date',
                    icon: 'warning',
                    timer: 5000
                })
            event.preventDefault()
        }
        if(sDateObj<todayDateObj){
            Swal.fire({
                    title: 'Error!',
                    text: 'Starting date should be abouve todays date',
                    icon: 'warning',
                    timer: 5000
                })
            
            event.preventDefault()
        }
    }

    function validateForm(){
        
        document.getElementById('err-msg').innerHTML = ''
        let name = document.getElementById('name').value;
        const nameRegex = /^(?!\s*$).+/;

        if(nameRegex.test(name)){
            document.getElementById('name').style.borderColor = 'white'
        }else{  
          document.getElementById('name').style.borderColor = 'red'
          document.getElementById('name').style.borderWidth = '1px'
          document.getElementById('err-msg').innerHTML = document.getElementById('err-msg').innerHTML + '<br> Name error'
          //alert("Name Eroor 1:Start with capital letter 2:No white spaces 3:No Special symbols")
          event.preventDefault()
        }

       
    }
  </script>